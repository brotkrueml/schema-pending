{
	"name": "brotkrueml/schema-pending",
	"description": "Extend the schema extension with pending terms",
	"license": "GPL-2.0-or-later",
	"type": "typo3-cms-extension",
	"keywords": [
		"TYPO3 CMS",
		"TYPO3 extension",
		"JSON-LD",
		"Structured data",
		"SEO",
		"schema.org"
	],
	"authors": [
		{
			"name": "Chris MÃ¼ller",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/brotkrueml/schema-pending/issues",
		"forum": "https://github.com/brotkrueml/schema/discussions",
		"source": "https://github.com/brotkrueml/schema-pending",
		"docs": "https://docs.typo3.org/p/brotkrueml/schema-pending/main/en-us/"
	},
	"require": {
		"php": ">=7.2",
		"ext-json": "*",
		"brotkrueml/schema": "^1.7 || ^2.0",
		"typo3/cms-core": "^9.5.16 || ^10.4 || ^11.5"
	},
	"require-dev": {
		"brotkrueml/coding-standards": "~2.0.2",
		"ergebnis/composer-normalize": "~2.18.0"
	},
	"autoload": {
		"psr-4": {
			"Brotkrueml\\SchemaPending\\": "Classes/"
		}
	},
	"config": {
		"allow-plugins": {
			"ergebnis/composer-normalize": true,
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true
		},
		"bin-dir": ".Build/bin",
		"sort-packages": true,
		"vendor-dir": ".Build/vendor"
	},
	"extra": {
		"branch-alias": {
			"dev-main": "1.3.x-dev"
		},
		"typo3/cms": {
			"app-dir": ".Build",
			"extension-key": "schema_pending",
			"web-dir": ".Build/web"
		}
	}
}
